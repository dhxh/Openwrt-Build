
config smartdns
	option dualstack_ip_selection '1'
	option prefetch_domain '1'
	option serve_expired '1'
	option seconddns_no_dualstack_selection '0'
	option coredump '0'
	option port '6053'
	option redirect 'dnsmasq-upstream'
	option seconddns_enabled '1'
	option seconddns_no_rule_addr '0'
	option seconddns_no_rule_nameserver '0'
	option seconddns_no_rule_ipset '0'
	option seconddns_no_rule_soa '0'
	option rr_ttl_min '5'
	option enabled '1'
	option server_name 'cn'
	option tcp_server '1'
	option ipv6_server '1'
	option cache_size '12000'
	option seconddns_port '7053'
	option seconddns_tcp_server '1'
	option seconddns_server_group 'gfw'
	option seconddns_no_speed_check '1'
	option seconddns_no_cache '1'
	option rr_ttl_max '1800'
	option force_aaaa_soa '1'
	list old_redirect 'dnsmasq-upstream'
	list old_port '6053'
	list old_enabled '1'

config server
	option enabled '1'
	option name 'AliDNS'
	option server_group 'cn'
	option blacklist_ip '0'
	option ip '223.5.5.5'
	option type 'udp'

